{
  "name": "archiyou",
  "type": "module",
  "version": "0.1.0",
  "description": "Execute Archiyou scripts in different contexts",
  "scripts": {
    "link" : "npm link archiyou",
    "dev": "node --loader ts-node/esm --no-warnings=ExperimentalWarning ./src/index.ts",
      "dev:basic": "node --loader ts-node/esm --no-warnings=ExperimentalWarning ./src/basic.ts",
      "dev:runner": "node --loader ts-node/esm --no-warnings=ExperimentalWarning ./src/runner.ts",
      "dev:pdf": "node --loader ts-node/esm --no-warnings=ExperimentalWarning ./src/pdf.ts",
      "dev:component": "node --loader ts-node/esm --no-warnings=ExperimentalWarning ./src/component.ts",
      "dev:library": "node --loader ts-node/esm --no-warnings=ExperimentalWarning ./src/library.ts",
      "dev:fromurl": "node --loader ts-node/esm --no-warnings=ExperimentalWarning ./src/fromurl.ts",
      "dev:debug": "node --loader ts-node/esm --no-warnings=ExperimentalWarning ./src/debugscript.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "overrides": {
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "typescript": "^4.9.5"
  }
}
